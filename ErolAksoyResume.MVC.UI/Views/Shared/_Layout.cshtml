<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ErolAksoyResume.MVC.UI</title>

    <!-- Simple Page Needs
    ================================================== -->
    <title>Mat</title>
    <meta charset="UTF-8">
    <meta name="description" content="Mat - Resume & vCard HTML Template">
    <meta name="keywords" content="personal, vcard, portfolio">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Favicon
    ================================================== -->
    <link href="favicon.ico" rel="shortcut icon" type="image/x-icon" />

    <!-- CSS
    ================================================== -->
    <link rel="stylesheet" href="~/home-content/css/bootstrap.css" />
    <link rel="stylesheet" href="~/home-content/css/reset.css" />
    <link rel="stylesheet" href="~/home-content/cubeportfolio/css/cubeportfolio.min.css" />
    <link rel="stylesheet" href="~/home-content/css/owl.theme.css" />
    <link rel="stylesheet" href="~/home-content/css/owl.carousel.css" />
    <link rel="stylesheet" href="~/home-content/css/style.css" />
    <link rel="stylesheet" href="~/home-content/css/colors/yellow.css" id="color" />


    <!-- Google Web fonts
    ================================================== -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,500,700,900" rel="stylesheet">

    <!-- Font Icons
    ================================================== -->
    <link rel="stylesheet" href="~/home-content/icon-fonts/font-awesome-4.7.0/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/home-content/icon-fonts/web-design/flaticon.css" />

    <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

</head>


<body>
    <!-- Preloading -->
    <div id="preloader">
        <div class="spinner">
        </div>
    </div>

    <!-- Wrapper -->
    <div class="wrapper top_60 container">
        <div class="row">

            <!-- Profile Section
            ================================================== -->
            <div class="col-lg-3 col-md-4">
                <div class="profile">
                    <div class="profile-name">
                        <span class="name">Henry Rooney</span><br>
                        <span class="job">Creative Designer</span>
                    </div>
                    <figure class="profile-image">
                        <img src="~/home-content/images/profile.jpg" alt="">
                    </figure>
                    <ul class="profile-information">
                        <li></li>
                        <li><p><span>Name:</span> Chris Johnson</p></li>
                        <li><p><span>Birthday:</span> 06 December 1987</p></li>
                        <li><p><span>Job:</span> Freelancer</p></li>
                        <li><p><span>Email:</span> henry@domain.com</p></li>
                        <li><p><span>Skype:</span> henryrooney85</p></li>
                    </ul>
                    <div class="col-md-12">
                        <button class="site-btn icon">Download Cv <i class="fa fa-download" aria-hidden="true"></i></button>
                    </div>
                </div>
            </div>

            <!-- Page Tab Container Div -->
            <div id="ajax-tab-container" class="col-lg-9 col-md-8 tab-container">

                <!-- Header
                ================================================== -->
                <div class="row">
                    <header class="col-md-12">
                        <nav>
                            <div class="row">
                                <!-- navigation bar -->
                                <div class="col-md-8 col-sm-8 col-xs-4">
                                    <ul class="tabs">
                                        <li class="tab">
                                            <a class="home-btn" asp-action="Index" ><i class="fa fa-home" aria-hidden="true"></i></a>
                                        </li>
                                        <li class="tab"><a asp-action="Resume">RESUME</a></li>
                                        <li class="tab"><a href="#portfolio">PORTFOLIO</a></li>
                                        <li class="tab"><a href="#blog">BLOG</a></li>
                                        <li class="tab"><a href="#contact">CONTACT</a></li>
                                    </ul>
                                </div>
                                <div class="col-md-4 col-sm-4 col-xs-8 dynamic">
                                    <button class="pull-right site-btn icon hidden-xs">Hire Me <i class="fa fa-paper-plane" aria-hidden="true"></i></button>
                                    <div class="hamburger pull-right hidden-lg hidden-md"><i class="fa fa-bars" aria-hidden="true"></i></div>
                                    <div class="hidden-md social-icons pull-right">
                                        <a class="fb" href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a>
                                        <a class="tw" href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a>
                                        <a class="ins" href="#"><i class="fa fa-instagram" aria-hidden="true"></i></a>
                                        <a class="dr" href="#"><i class="fa fa-dribbble" aria-hidden="true"></i></a>
                                    </div>
                                </div>
                            </div>
                        </nav>
                    </header>
                    <div class="col-md-12">
                        <div id="content" class="panel-container">

                            @RenderBody()

                        </div>
                    </div>
                    <footer>
                        <div class="footer col-md-12 top_30 bottom_30">
                            <div class="name col-md-4 hidden-md hidden-sm hidden-xs">Erol Aksoy</div>

                        </div>
                    </footer>

                </div> <!-- Tab Container - End -->
            </div> <!-- Row - End -->
        </div> <!-- Wrapper - End -->
        <!-- Javascripts
        ================================================== -->
        <script src="~/home-content/js/jquery-2.1.4.min.js"></script>
        <script src="~/home-content/cubeportfolio/js/jquery.cubeportfolio.min.js"></script>
        <script src="~/home-content/js/bootstrap.min.js"></script>
        <script src="~/home-content/js/jquery.easytabs.min.js"></script>
        <script src="~/home-content/js/owl.carousel.min.js"></script>
        <script src="~/home-content/js/main.js"></script>
        <!-- for color alternatives -->
        <script src="~/home-content/js/jquery.cookie-1.4.1.min.js"></script>
        <script src="~/home-content/js/Demo.js"></script>
        <link rel="stylesheet" href="~/home-content/css/Demo.min.css" />
        @RenderSection("JavaScript", false)
</body>
</html>
