@model AppUserViewComponentDto
<!-- Profile Section
      ================================================== -->

<div class="col-lg-3 col-md-4">
    <div class="profile">
        <div class="profile-name">
            <span class="name">@Model.Name @Model.LastName </span><br>
            <span class="job">@Model.Status</span>
        </div>
        <figure class="profile-image">
            <img src="~/img/profile/@Model.ProfileImageUrl" alt="@(Model.Name + "ProfileImage")">
        </figure>
        <ul class="profile-information">
            <li></li>
            <li><p><span>Name:</span> @Model.Name @Model.LastName</p></li>
            <li><p><span>Birthday:</span> Birhtday</p></li>
            <li><p><span>Job:</span> @Model.Status</p></li>
            <li><p><span>Email:</span> @Model.Email</p></li>
            <li><p><span>Skype:</span> henryrooney85</p></li>
        </ul>
        <div class="col-md-12">
            <button class="site-btn icon">Download Cv <i class="fa fa-download" aria-hidden="true"></i></button>
        </div>
    </div>
</div>

<!-- Page Tab Container Div -->
<div id="ajax-tab-container" class="col-lg-9 col-md-8 tab-container">

    <!-- Header
    ================================================== -->
    <div class="row">
        <header class="col-md-12">
            <nav>
                <div class="row">
                    <!-- navigation bar -->
                    <div class="col-md-8 col-sm-8 col-xs-4">
                        <ul class="tabs">
                            <li class="tab">
                                <a class="home-btn" asp-action="Index"><i class="fa fa-home" aria-hidden="true"></i></a>
                            </li>
                            <li class="tab"><a asp-action="Resume">RESUME</a></li>
                            <li class="tab"><a asp-action="Portofolio">PORTFOLIO</a></li>
                            <li class="tab"><a asp-action="Blog" asp-route-id="">BLOG</a></li>
                            <li class="tab"><a href="#contact">CONTACT</a></li>
                        </ul>
                    </div>
                    <div class="col-md-4 col-sm-4 col-xs-8 dynamic">
                        <button class="pull-right site-btn icon hidden-xs">Hire Me <i class="fa fa-paper-plane" aria-hidden="true"></i></button>
                        <div class="hamburger pull-right hidden-lg hidden-md"><i class="fa fa-bars" aria-hidden="true"></i></div>
                        <div class="hidden-md social-icons pull-right">
                            <a class="fb" href="http://facebook.com/@Model.Facebook"><i class="fa fa-facebook" aria-hidden="true"></i></a>
                            <a class="tw" href="http://twitter.com/@Model.Twitter"><i class="fa fa-twitter" aria-hidden="true"></i></a>
                            <a class="ins" href="http://instagram.com/@Model.Instagram"><i class="fa fa-instagram" aria-hidden="true"></i></a>
                            <a class="dr" href="#"><i class="fa fa-dribbble" aria-hidden="true"></i></a>
                        </div>
                    </div>
                </div>
            </nav>
        </header>
        
        

    </div> <!-- Tab Container - End -->
</div> <!-- Row - End -->

